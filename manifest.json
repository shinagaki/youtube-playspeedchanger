{
	"name": "YouTube Playback Speed",
	"version": "1.1.0",
	"manifest_version": 3,
	"description": "YouTube の動画読み込み時に再生速度を変更します",
	"host_permissions": ["https://www.youtube.com/*"],
	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/*"],
			"js": ["content.js"]
		}
	],
	"permissions": ["storage"],
	"options_page": "options.html"
}
